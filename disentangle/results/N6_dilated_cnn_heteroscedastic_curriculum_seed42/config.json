{
  "architecture": "dilated_cnn",
  "sequence_length": 230,
  "hidden_dim": 256,
  "n_filters_dilated": 256,
  "n_dilated_layers": 6,
  "dilation_base": 2,
  "dropout": 0.1,
  "condition": "heteroscedastic_mse",
  "loss": "heteroscedastic",
  "learning_rate": 0.001,
  "weight_decay": 1e-05,
  "batch_size": 128,
  "max_epochs": 100,
  "early_stopping_patience": 15,
  "scheduler": "cosine",
  "warmup_epochs": 5,
  "gradient_clip_norm": 1.0,
  "predict_variance": true,
  "use_heteroscedastic": true,
  "heteroscedastic_beta": 0.5,
  "use_replicate_std_prior": false,
  "replicate_std_prior_weight": 0.1,
  "augmentation": "rc_mixup",
  "use_noise_curriculum": true,
  "output_dir": "results/N6_dilated_cnn_heteroscedastic_curriculum_seed42",
  "seed": 42,
  "data_files": [
    "data/processed/dream_K562.h5",
    "data/processed/dream_HepG2.h5"
  ],
  "paired_data": "data/processed/paired_K562_HepG2.h5"
}